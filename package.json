{
  "scripts": {
    "test": "jest && eslint ./src/*.js",
    "start": "webpack-dev-server",
    "build": "webpack --mode=development",
    "predeploy": "npm run build",
    "deploy": "gh-pages-deploy",
    "lint": "npm run lint:js && npm run lint:dup",
    "lint:check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:js": "eslint src/components/*.js -c .eslintrc.json",
    "lint:dup": "jscpd -g --threshold=1 tests docker"
  },
  "name": "fronttestmail",
  "homepage": "https://mpolyakowa.github.io/FrontTestMailru/",
  "version": "1.0.0",
